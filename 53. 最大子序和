class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        if(nums.empty()) return 0;
        
        int res = INT_MIN;
        int f_n = -1;
        for(int i = 0; i < nums.size(); i++)
        {
            f_n = max(nums[i], f_n + nums[i]);
            res = max(res, f_n);
        }
        return res;
    }
};
